{
    "name": "inoplate/media",
    "description": "Inoplate media manager",
    "keywords": ["laravel", "inoplate", "media-manager"],
    "type": "composer",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inoplate\\Media\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "require": {
        "ramsey/uuid": "^3.1",
        "inoplate/notifier" : "dev-master",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3"
    },
    "config": {
        "preferred-install": "dist"
    }
}
